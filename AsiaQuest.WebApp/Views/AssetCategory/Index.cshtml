
@{
	ViewData["Title"] = "Index";
}
<h3>Asset Category List</h3>
<table id="tableAssetCategory" class="table table-striped table-bordered table-hover nowrap" style="width:100%">
	<thead class="bg-primary">
		<tr>
			<th class="text-light">Asset Name</th>
			<th class="text-light">Modified Date</th>

		</tr>
	</thead>
</table>
<script>
	$(document).ready(function () {
		datatables.init();
	});

	var datatables = {
		init: function () {
			try {
				var url = "/AssetCategory/GetAssetCategories";
				var datatable = $("#tableAssetCategory").DataTable({
					ajax: url,
					columns: [
						{
							data: "name"
						},
						{
							data: "modifiedDateString"
						},
					],
					scrollX: true
				});
			} catch (error) {
				console.log(error);
			}
		}
	}
</script>